<permissions>
  <niagara-permission-groups type='all'>
    <req-permission>
      <name>NETWORK_COMMUNICATION</name>
      <purposeKey>This module needs it.</purposeKey>
      <parameters>
        <parameter name="hosts" value="*" />
        <parameter name="ports" value="*" />
      </parameters>
    </req-permission>
    <req-permission>
      <name>SYSTEM_PROPERTIES</name>
      <purposeKey>This module needs it.</purposeKey>
      <parameters>
        <parameter name="properties" value="*" />
        <parameter name="actions" value="read,write" />
      </parameters>
    </req-permission>
    <req-permission>
      <name>GET_ENVIRONMENT_VARIABLES</name>
      <purposeKey>This module needs it.</purposeKey>
      <parameters>
        <parameter name="variables" value="*" />
      </parameters>
    </req-permission>
    <req-permission>
      <name>LOAD_LIBRARIES</name>
      <purposeKey>This module needs to load dll.</purposeKey>
      <parameters>
        <parameter name="libraries" value="*,sqlitejdbc"/>
      </parameters>
    </req-permission>
    <req-permission>
      <name>MANAGE_EXECUTION</name>
      <purposeKey>This module needs it.</purposeKey>
    </req-permission>
    <req-permission>
      <name>LOGGING</name>
      <purposeKey>This module needs it.</purposeKey>
    </req-permission>
    <req-permission>
      <name>MODIFY_IO_STREAMS</name>
      <purposeKey>This module needs it.</purposeKey>
    </req-permission>
    <req-permission>
      <name>SHUTDOWN_HOOKS</name>
      <purposeKey>This module needs it.</purposeKey>
    </req-permission>
  </niagara-permission-groups>
</permissions>